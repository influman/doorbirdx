{
	"plugin_id": "doorbirdx",
	"name_fr": "DoorbirdX",
	"icon": "doorbirdx.png",
	"version": "1.0",
	"creation_date": "2018-03-20",
	"modification_date": "2018-03-21",
	"author": "Influman",
	"description_fr": "Autre gestion du portier connecté <a href='http://www.doorbird.com' target='_blank'>Doorbird</a><br>Plugin adapté à l'utilisation du Doorbird D2101V avec firmware 000110 ou supérieur (nouvelle API)",
	"description_en": "",
	"description_es": "",
	"description_it": "",
	"description_de": "",
	"forum_link": "https://forum.eedomus.com/viewtopic.php?f=17&t=6870",
	"parameters": 
	[
		{
			"parameter": "NAME", 
			"description": "Appareil",
			"xtype": "textfield",
			"default_value": "Portier",
			"field": "width:130, allowBlank:false"
		},
		{
			"parameter": "IP", 
			"description": "IP locale du Doorbird",
			"xtype": "textfield",
			"default_value": "192.168.0.",
			"field": "width:130, allowBlank:false, maskRe:/[0-9.:]/, regex:/^(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9])\\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9]|0)\\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9]|0)\\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[0-9])$/, regexText:'Adresse IP invalide [0-255].[0-255].[0-255].[0-255]'"
		},
		{
			"parameter": "LOGIN", 
			"description": "Identifiant",
			"xtype": "textfield",
			"default_value": ".....0001",
			"field": "width:130, allowBlank:false"
		},
		{
			"parameter": "PWD", 
			"description": "Mot de passe",
			"xtype": "textfield",
			"default_value": "",
			"field": "width:130, allowBlank:false"
		},
		{
			"parameter": "line1",
			"description": "Paramètres FTP (Camera eedomus)",
			"xtype": "displayfield",
			"field": "style: 'padding: 3px;', value: '......................................'"
		},	
		{
            "parameter": "FTPSERV", 
            "description": "Server : ",
            "xtype": "textfield",
			"default_value": "camera.eedomus.com",
			"field": "width:150, allowBlank:true"
		},
		{
            "parameter": "FTPUSR", 
            "description": "User : ",
            "xtype": "textfield",
			"default_value": "",
			"field": "width:150, allowBlank:true"
		},
		{
            "parameter": "FTPPWD", 
            "description": "Password : ",
            "xtype": "textfield",
			"default_value": "",
			"field": "width:150, allowBlank:true"
		},
		{
			"parameter": "line2",
			"description": "Vos paramètres API eedomus",
			"xtype": "displayfield",
			"field": "style: 'padding: 3px;', value: '......................................'"
		},	
		{
            "parameter": "APIU", 
            "description": "Code API User : ",
            "xtype": "textfield",
			"default_value": "NNNNNN",
			"field": "width:150, allowBlank:false"
		},
		{
            "parameter": "APIS", 
            "description": "Code API Secret : ",
            "xtype": "textfield",
			"default_value": "",
			"field": "width:150, allowBlank:false"
		}
	 ],
	"scripts":
	[
		{
			"name": "doorbirdx.php"
		}
	],
	"devices":
